[messages]
logmon_nofilename = no file to tail
logmon_parserr    = unable to parse "%s"
logmon_badzone    = invalid time zone passed from %s at line %s for zone "%s"
logmon_notifier   = unable to initialize notifier; reason: %s
logmon_baddate    => error: "%s" for date: "%s"
